
RewriteEngine On
Options All -indexes
RewriteRule ^([a-zA-Z0-9/-]+)/([a-zA-Z0-9/-]+)/([a-zA-Z0-9/-]+)$ index.php?controlador=$1&accion=$2&id=$3 [L,QSA]
RewriteRule ^([a-zA-Z0-9/-]+)/([a-zA-Z0-9/-]+)$ index.php?controlador=$1&accion=$2 [L,QSA]
RewriteRule ^([a-zA-Z0-9/-]+)$ index.php?controlador=$1 [L,QSA]
